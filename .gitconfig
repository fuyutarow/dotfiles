[url "://danielkummer.github.io/git-flow-cheatsheet/ttps://"]
	insteadOf = git://

[user]
  email = sktnkysh+dev@gmail.com
  name = sktnkysh
  
[url "github:"]
  pushInsteadOf = https://github.com/
  pushInsteadOf = git@github.com:
  
[alias]
  alias = !git config --get-regexp '^alias\\.' | sed 's/alias\\.\\([^ ]*\\) \\(.*\\)/\\1\\\t => \\2/' | sort
  a = add
  ap = add -p
  amend = commit --amend 
  acm = !git add -A && git commit -m 
  b = branch
  ba = branch -a
  bd = branch -d
  bm = branch -m #old_name #new_name
  ca =! git add . && git commit --amend
  cb = checkout -b
  co = checkout
  cm = commit -m 
  c-help= !echo 'https://qiita.com/muran001/items/f13742b51da3a22117ee'
  d = diff
  dc = git diff --cached
  diff-help = !echo 'http://dqn.sakusakutto.jp/2013/06/git_diff_branch_file.html'
  f = fetch
  g = log --all --graph --oneline --decorate=full
  l = ls-files
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
  m = merge --no-ff 
  pull-force = !git fetch origin & git reset --hard #origin/master
  s = status
  ss = status -s
  stash-help = !echo 'https://qiita.com/akasakas/items/768c0b563b96f8a9be9d'
  ta = log --graph --branches --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(green)- %an, %cr%Creset'
  r = remote -v
  reset = reset --hard HEAD^
  undo = reset HEAD~1 --mixed
  ignore =! git rm -r --cached . & git add .
  # cp-file-over-branch = !git co <dst_branch> & git co <src_branch> -- <src_file>


# git flow
  flow-init = flow init
  flow-help = !sh -c 'echo https://danielkummer.github.io/git-flow-cheatsheet/ && echo https://gist.github.com/JamesMGreene/cdd0ac49f90c987e45ac'
  echoes = !sh -c 'echo hi && echo bye'

  ff = flow feature
  ffs = flow feature start
  ffp = flow feature publish
  
[push]
  default = simple
